{
  "name": "pingmc",
  "version": "3.1.0",
  "author": "MrZillaGold <Pronin Egor | mrzillagold@gmail.com>",
  "description": "Promise-based Minecraft pinger in Node.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "postbuild": "node --experimental-modules --es-module-specifier-resolution=node ./scripts/cleanUpBuild.mjs && babel build --out-dir dist --copy-files",
    "eslint:check": "eslint ./src/**/*",
    "eslint:fix": "eslint ./src/**/* --fix"
  },
  "main": "./dist/PingMC.js",
  "exports": {
    ".": [
      {
        "require": "./dist/PingMC.js",
        "import": "./esm/PingMC.mjs"
      },
      "./dist/PingMC.js"
    ],
    "./esm": "./esm/PingMC.mjs"
  },
  "types": "./dist/interfaces/index.d.ts",
  "module": "esm/PingMC.mjs",
  "keywords": [
    "ping",
    "minecraft",
    "server",
    "query",
    "srv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrZillaGold/PingMC"
  },
  "dependencies": {
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@types/node": "^18.11.13",
    "@types/node-int64": "^0.4.29",
    "@types/varint": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "typescript": "^4.2.3"
  }
}
